{
  "name": "etlok/crux-websockets",
  "description": "Crux WebSockets Module.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Akshay Kolte",
      "email": "akshay.kolte@etlok.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Etlok\\Crux\\WebSockets\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Etlok\\Crux\\WebSockets\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Etlok\\Crux\\WebSockets\\CruxWebSocketsServiceProvider"
      ]
    }
  },
  "repositories": [ { "type": "composer", "url": "https://packages.etlokstudios.com/" } ],
  "require": {
    "etlok/crux": "1.*",
    "beyondcode/laravel-websockets": "^1.13",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.6"
  }
}
